 "feedstock_name": "openfe",
 "linux_64_meta_yaml": {
  "about": {
   "description": "The Open Free Energy (OpenFE) project is dedicated to the maintenance and\ndevelopment of open source tools for free energy calculations to guide\npharmaceutical drug design and discovery. This package enables researchers\nto plan, execute, and analyze networks of free energy calculations.\n",
   "dev_url": "https://github.com/OpenFreeEnergy/openfe",
   "doc_url": "https://openfe.rtfd.io/",
   "home": "https://github.com/OpenFreeEnergy/openfe",
   "license": "MIT",
   "license_family": "MIT",
   "license_file": "LICENSE",
   "summary": "Free software for Free Energies."
  },
  "build": {
   "entry_points": [
    "openfe = openfecli.cli:main"
   ],
   "noarch": "python",
   "number": "2",
   "script": "PYTHON -m pip install . -vv --no-deps"
  },
  "extra": {
   "recipe-maintainers": [
    "mikemhenry",
    "richardjgowers",
    "IAlibay",
    "dwhswenson"
   ]
  },
  "package": {
   "name": "openfe",
   "version": "1.0.1"
  },
  "requirements": {
   "host": [
    "pip",
    "python >=3.9",
    "versioningit"
   ],
   "run": [
    "cinnabar ~=0.4.0",
    "click",
    "gufe ~=1.0",
    "kartograf ~=1.0",
    "lomap2 ~=3.0",
    "matplotlib-base <3.9.0",
    "networkx",
    "numpy",
    "openfe-analysis ~=0.2.0",
    "openff-toolkit >=0.13,<0.16",
    "openmm !=8.1.0",
    "openmmforcefields <0.15",
    "openmmtools ~=0.23.0",
    "packaging",
    "plugcli",
    "pooch",
    "py3dmol",
    "pygraphviz",
    "pymbar ~=3.0",
    "pytest",
    "python >=3.9",
    "rdkit",
    "typing-extensions"
   ]
  },
  "source": {
   "git_depth": "-1",
   "git_rev": "v1.0.1",
   "git_url": "https://github.com/OpenFreeEnergy/openfe.git",
   "patches": [
    "patches/fix-gaff-tests.patch"
   ]
  },
  "test": {
   "commands": [
    "openfe --help",
    "openfe --version",
    "pytest -v -n auto --pyargs openfe openfecli"
   ],
   "imports": [
    "openfe"
   ],
   "requires": [
    "pip",
    "pytest",
    "pytest-xdist",
    "openmmforcefields=0.12"
   ]
  }
 },
