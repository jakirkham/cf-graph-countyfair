 "linux_64_meta_yaml": {
  "about": {
   "description": "scikit-learn-extra is a Python module for machine learning that extends\nscikit-learn. It includes algorithms that are useful but do not satisfy\nthe scikit-learn inclusion criteria, for instance due to their novelty or\nlower citation number.\n",
   "dev_url": "https://github.com/scikit-learn-contrib/scikit-learn-extra",
   "doc_url": "https://scikit-learn-extra.readthedocs.io/",
   "home": "https://github.com/scikit-learn-contrib/scikit-learn-extra",
   "license": "BSD-3-Clause",
   "license_family": "BSD",
   "license_file": "LICENSE",
   "summary": "scikit-learn contrib estimators"
  },
  "build": {
   "number": "0",
   "script": "PYTHON -m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "rth"
   ]
  },
  "package": {
   "name": "scikit-learn-extra",
   "version": "0.3.0"
  },
  "requirements": {
   "build": [
    "c_stdlib_stub",
    "c_compiler_stub",
    "cxx_compiler_stub"
   ],
   "host": [
    "python",
    "pip",
    "cython >=0.28.5",
    "numpy"
   ],
   "run": [
    "python",
    "numpy",
    "scipy",
    "joblib",
    "scikit-learn >=0.23.0,<1.3.0"
   ]
  },
  "source": {
   "sha256": "7912d43384470d77609e53819d0291967f21010e5c3a3847f9026fc02f9c7893",
   "url": "https://pypi.io/packages/source/s/scikit-learn-extra/scikit-learn-extra-0.3.0.tar.gz"
  },
  "test": {
   "commands": [
    "export OPENBLAS_NUM_THREADS=1",
    "export OMP_NUM_THREADS=1",
    "pytest --verbose --pyargs sklearn_extra -k \"not test_all_estimators\""
   ],
   "imports": [
    "sklearn_extra"
   ],
   "requires": [
    "pytest >=3.3.0,<8"
   ]
  }
 },
