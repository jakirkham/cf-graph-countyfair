  "about": {
   "description": "This library provides well-tested Python bindings for libsecp256k1,\nthe heavily optimized C library used by Bitcoin Core for operations\non the elliptic curve secp256k1.\n",
   "doc_url": "https://ofek.dev/coincurve/",
   "home": "https://github.com/ofek/coincurve",
   "license": "MIT OR Apache-2.0",
   "license_file": [
    "LICENSE-MIT",
    "LICENSE-APACHE"
   ],
   "summary": "Cross-platform Python CFFI bindings for libsecp256k1"
  },
  "build": {
   "number": "0",
   "script": "PYTHON -m pip install . --use-pep517 -vvv",
   "script_env": [
    "COINCURVE_IGNORE_SYSTEM_LIB='OFF'",
    "COINCURVE_UPSTREAM_REF='__no_upstream__'"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "ofek",
    "MementoRC"
   ]
  },
  "package": {
   "name": "coincurve",
   "version": "20.0.0"
  },
  "requirements": {
   "build": [
    "c_compiler_stub",
    "c_stdlib_stub",
    "cmake >=3.15",
    "make",
    "python"
   ],
   "host": [
    "cffi >=1.3.0",
    "hatchling >=1.24.2",
    "libsecp256k1",
    "pip",
    "pkg-config",
    "python",
    "requests",
    "scikit-build-core",
